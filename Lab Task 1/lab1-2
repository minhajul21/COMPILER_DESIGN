#include <iostream>
using namespace std;

int main() {
    char op;
    cout << "Enter an operator: ";
    cin >> op;

    if (op == '+' || op == '-' || op == '*' || op == '/' || op == '%')
        cout << "Arithmetic Operator" << endl;
    else
        cout << "Not an Arithmetic Operator" << endl;

    return 0;
}
